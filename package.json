  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "rm-cache": "rm -rf public && rm -rf .cache",
    "build-prefix": "gatsby build --prefix-paths",
    "deploy": "yarn rm-cache && yarn format && yarn build-prefix && gh-pages -d public -m 'autodeploy [ci skip]'",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'"
