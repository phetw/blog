{
  "name": "wasuwat-personal-blog",
  "description": "My personal blog - powered by Gatsby",
  "version": "2.0.0",
  "author": "Wasuwat Limsuparhat <wasuwat.se@gmail.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "npm run format",
      "git add"
    ]
  },
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "lh": "lighthousebot",
    "rm-cache": "rm -rf public && rm -rf .cache",
    "build-prefix": "gatsby build --prefix-paths",
    "deploy": "yarn rm-cache && yarn format && yarn build-prefix && gh-pages -d public -m 'autodeploy [ci skip]'",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.5.2",
    "gatsby-image": "^2.1.0",
    "gatsby-plugin-catch-links": "^2.0.14",
    "gatsby-plugin-google-analytics": "^2.0.20",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-netlify": "^2.0.6",
    "gatsby-plugin-netlify-cms": "^4.0.1",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.0.37",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typography": "^2.2.13",
    "gatsby-remark-copy-linked-files": "^2.0.7",
    "gatsby-remark-images": "^3.0.11",
    "gatsby-remark-prismjs": "^3.2.8",
    "gatsby-remark-relative-images": "^0.2.1",
    "gatsby-source-filesystem": "^2.0.36",
    "gatsby-transformer-remark": "^2.3.12",
    "gatsby-transformer-sharp": "^2.1.18",
    "netlify-cms-app": "^2.9.1",
    "prismjs": "^1.15.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.19",
    "styled-components": "^4.2.0",
    "typography": "^0.16.19",
    "typography-theme-grand-view": "^0.16.19"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.15.3"
  }
}
